{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:bbohen/react-redux-boilerplate.git",
  "author": "Brad Bohen <bbohen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "webpack-dev-server --open --history-api-fallback",
    "lint": "eslint src --ext .js --fix",
    "test": "jest",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.3",
    "react-redux": "^5.0.2",
    "react-router": "4.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-pack": "^0.1.4",
    "redux-thunk": "^2.2.0",
    "styled-components": "^1.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^18.1.0",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.1",
    "webpack": "beta",
    "webpack-dev-server": "2.1.0-beta.12"
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.test\\.js$",
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleNameMapper": {
      "^components": "<rootDir>/src/components",
      "^containers": "<rootDir>/src/containers",
      "^theme": "<rootDir>/src/theme.js"
    }
  }
}
