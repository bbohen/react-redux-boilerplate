{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:bbohen/react-redux-boilerplate.git",
  "author": "Brad Bohen <bbohen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "webpack-dev-server --open --history-api-fallback",
    "lint": "eslint src __tests__ --ext .js --fix",
    "test": "jest",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "offline-plugin": "^4.6.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.3",
    "react-redux": "^5.0.2",
    "react-router": "4.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-pack": "^0.1.4",
    "redux-thunk": "^2.2.0",
    "styled-components": "^1.4.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^18.1.0",
    "jest-fetch-mock": "1.0.6",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./__tests__/testUtils/setup.js"
    ],
    "testRegex": "/__tests__/.*\\.test\\.js$",
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleNameMapper": {
      "^components": "<rootDir>/src/components",
      "^containers": "<rootDir>/src/containers",
      "^theme": "<rootDir>/src/theme.js"
    }
  }
}
