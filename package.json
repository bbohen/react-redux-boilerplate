{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:bbohen/react-redux-boilerplate.git",
  "author": "Brad Bohen <bbohen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "webpack-dev-server --open --history-api-fallback",
    "lint": "eslint src __tests__ --ext .js --fix",
    "test": "jest",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "offline-plugin": "^4.8.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "react-redux": "^5.0.5",
    "react-router": "4.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-pack": "^0.1.5",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "jest-fetch-mock": "^1.2.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./__tests__/testUtils/setup.js"
    ],
    "testRegex": "/__tests__/.*\\.test\\.js$",
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleNameMapper": {
      "^components": "<rootDir>/src/components",
      "^containers": "<rootDir>/src/containers",
      "^theme": "<rootDir>/src/theme.js"
    }
  }
}
